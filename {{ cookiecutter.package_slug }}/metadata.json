{
    "title": "{{ cookiecutter.model_name }}",
    "summary": "{{ cookiecutter.description.replace('\'', '') }}",
    "description": [
        "{{ cookiecutter.model_name }} is an application using the DEEPaaS API.\n",
        "Write additional information for your users (how to predict, how to retrain,",
        "dataset description, training description, etc)."
    ],
    "keywords": [
        "docker",
        "api-v2"
    ],
    "license": "{{ cookiecutter.open_source_license }}",
    "date_creation": "{% now 'utc', '%Y-%m-%d' %}",
    "sources": {
        "dockerfile_repo": "{{ cookiecutter.git_base_url }}/{{ cookiecutter.__model_source }}",
        "docker_registry_repo": "{{ cookiecutter.dockerhub_user }}/{{ cookiecutter.__model_source }}",
        "code": "{{ cookiecutter.git_base_url }}/{{ cookiecutter.__model_source }}"
    },
    "continuous_integration": {
        "build_status_badge": "https://jenkins.indigo-datacloud.eu/buildStatus/icon?job=Pipeline-as-code/DEEP-OC-org/{{ cookiecutter.__model_source }}/master",
        "build_status_url": "https://jenkins.indigo-datacloud.eu/job/Pipeline-as-code/job/DEEP-OC-org/job/{{ cookiecutter.__model_source }}/job/master"
    },
    "tosca": [
        {
            "title": "Mesos (CPU)",
            "url": "https://raw.githubusercontent.com/indigo-dc/tosca-templates/master/deep-oc/deep-oc-marathon-webdav.yml",
            "inputs": [
                "rclone_conf",
                "rclone_url",
                "rclone_vendor",
                "rclone_user",
                "rclone_pass"
            ]
        }
    ]
}